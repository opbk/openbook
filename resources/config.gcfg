[db]
driver     = postgres
connection = postgres://developer:developer@localhost/openbook

[frontend]
httpport     = 8089
templatepath = build/frontend/usr/lib/openbook/frontend/templates
staticpath   = build/frontend/usr/lib/openbook/frontend/static
uploadpath   = build/frontend/var/lib/openbook/frontend/upload

[backend]
httpport     = 8090
templatepath = resources/backend/templates/

[main]
senddelay = 0.1s
logfile = seelog.xml
maxproc = 8